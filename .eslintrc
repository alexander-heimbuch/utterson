{
    "env": {
        "es6": true
    },
    "rules": {
        "indent": 2,
        "comma-style": [2, "last"],
        "no-cond-assign": 2,
        "comma-dangle": 2,
        "no-dupe-keys": 2,
        "no-extra-semi": 2,
        "no-debugger": 2,
        "no-unreachable": 2,
        "valid-jsdoc": [2, {
            "requireReturn": false
        }],
        "valid-typeof": 2,

        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "guard-for-in": 2,
        "no-caller": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-multi-str": 2,
        "no-new": 2,
        "no-new-wrappers": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-self-compare": 2,
        "no-with": 2,
        "wrap-iife": 2,

        "strict": [2, "global"],

        "no-shadow-restricted-names": 2,
        "no-undef": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,

        "no-path-concat": 2,
        "no-sync": 2,

        "brace-style": [2, "1tbs"],
        "camelcase": 2,
        "consistent-this": [2, "self"],
        "eol-last": 2,
        "no-lonely-if": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [2, {
            "max": 1
        }],
        "new-cap": [2, {
            "newIsCapExceptions": ["model"],
            "capIsNewExceptions": ["Router"]
        }],
        "new-parens": 2,
        "semi-spacing": 2,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 0,
        "no-mixed-spaces-and-tabs": 2,
        "quotes": [2, "single", "avoid-escape"],
        "semi": [2, "always"],
        "space-after-keywords": [2, "always"],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "space-unary-ops": [2, {
            "words": true,
            "nonwords": false
        }],
        "space-return-throw-case": 2,
        "space-before-function-paren": [2, {
            "anonymous": "always",
            "named": "never"
        }],
        "vars-on-top": 2,

        "no-bitwise": 2,
        "no-plusplus": 2
    }
}
